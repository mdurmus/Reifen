@model Reminder

    <div class="col-md-12">
        <!-- general form elements -->
        <div class="card card-primary">
            <div class="card-header">
                <h3 class="card-title">KR Hinzufügen</h3>
            </div>
            <!-- /.card-header -->
            <!-- form start -->
            <form asp-controller="Home" asp-action="CreatePark" method="post">
                <div class="card-body">
                    <div class="form-group">
                        <label asp-for="Name" class="control-label"></label>
                        <input asp-for="Name" class="form-control" />
                        <span asp-validation-for="Name" class="text-warning"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="Lastname" class="control-label"></label>
                        <input asp-for="Lastname" class="form-control" />
                        <span asp-validation-for="Lastname" class="text-warning"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="GSM" class="control-label"></label>
                        <input asp-for="GSM" class="form-control" />
                        <span asp-validation-for="GSM" class="text-warning"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="Email" class="control-label"></label>
                        <input asp-for="Email" class="form-control" />
                        <span asp-validation-for="Email" class="text-warning"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="Street" class="control-label"></label>
                        <input asp-for="Street" class="form-control" />
                        <span asp-validation-for="Street" class="text-warning"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="Postal" class="control-label"></label>
                        <input asp-for="Postal" class="form-control" />
                        <span asp-validation-for="Postal" class="text-warning"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="City" class="control-label"></label>
                        <input asp-for="City" class="form-control" />
                        <span asp-validation-for="City" class="text-warning"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="Plate" class="control-label"></label>
                        <input asp-for="Plate" class="form-control" />
                        <span asp-validation-for="Plate" class="text-warning"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="WareHouse" class="control-label"></label>
                        <select asp-for="WareHouse" class="form-control">
                            <option value="1" selected>1</option>
                            <option value="2">2</option>
                            <option value="3">3</option>
                            <option value="4">4</option>
                            <option value="5">5</option>
                        </select>
                        <span asp-validation-for="WareHouse" class="text-warning"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="Row" class="control-label"></label>
                        <input asp-for="Row" class="form-control" />
                        <span asp-validation-for="Row" class="text-warning"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="Column" class="control-label"></label>
                        <input asp-for="Column" class="form-control" />
                        <span asp-validation-for="Column" class="text-warning"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="EndDate" class="control-label"></label>
                        <input asp-for="EndDate" class="form-control" />
                        <span asp-validation-for="EndDate" class="text-warning"></span>
                    </div>
                </div>
                <div class="card-footer">
                    <button type="submit" class="btn btn-block btn-primary">Erstellen</button>
                </div>
            </form>
        </div>
    </div>
